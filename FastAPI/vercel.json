{
    "version": 2,
    "functions": {
        "api": {
            "runtime": "python@3.9"
        }
    },
    "routes": [
        {
            "src": "/transactions/(.*)",
            "dest": "/main.py"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/404.html"
        }
    ]
}
